def KaymeraRepositoryPath = "/mnt/data/projects/Kaymera_Apps"
def BuildScriptPath = KaymeraRepositoryPath + "/Kmtd/jenkins" 
node {
	stage ('starting'){
		dir (KaymeraRepositoryPath) {
			git credentialsId: '50192fae-78ac-46bc-b1ed-ae63a802c0a1', 
				url: 'http://gerrit02.kaymera.com:8080/KaymeraRepository.git',
				branch: 'master'
		}
	}
	stage ('build'){
		dir(BuildScriptPath)
			sh 'build.sh'
	}
}
